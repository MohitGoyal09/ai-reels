# Python bytecode
__pycache__/
*.py[cod]
*$py.class

# Distribution / packaging
dist/
build/
*.egg-info/

# Virtual environments
venv/
env/
ENV/
.env/

# IDE specific files
.idea/
.vscode/
*.swp
*.swo
.DS_Store

# Jupyter Notebook
.ipynb_checkpoints

# Logs
*.log

# Local development settings
.env
.env.local

# Temporary files
*.tmp
temp/
tmp/

# Keep data/ and output/ directories
# These directories will be committed to version control
!data/
!output/

# But ignore large model files inside data that shouldn't be committed
data/**/*.h5
data/**/*.ckpt
data/**/*.pt
data/**/*.pth
data/**/*.bin
data/**/*.onnx
data/**/*.pb

# Allow committing structure/placeholder files
!data/.gitkeep
!output/.gitkeep
